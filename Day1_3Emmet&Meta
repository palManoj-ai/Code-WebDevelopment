    <meta name="robots" content="INDEX, FOLLOW">
    <!-- above metatags are used in SEO, INDEX and FOLLOW setting helps in website ranking...-->
    <!-- ...NOINDEX, NOFOLLOW setting doesnt --> -->

    <link rel="stylesheet" href="style.css">
    <script src="style.js"></script>
    <!-- this is to include external links to css and js files, style is the name of the file -->
    <title>Webpage Title</title>
